<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>


<GridPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" fx:controller="com.mirre.cardgame.view.GameViewController">
   <rowConstraints>
      <RowConstraints minHeight="50.0" prefHeight="50.0" />
      <RowConstraints minHeight="200.0" prefHeight="200.0" />
      <RowConstraints minHeight="100.0" prefHeight="100.0" />
      <RowConstraints minHeight="200.0" prefHeight="200.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
      <ColumnConstraints minWidth="640.0" prefWidth="640.0" />
      <ColumnConstraints maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
   </columnConstraints>
   <children>
      <Label fx:id="mineLabel" text="Label" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="winLabel" text="Who wins?" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="otherLabel" text="Label" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="startButton" mnemonicParsing="false" onAction="#newMatch" text="Start" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
   </children>
</GridPane>
